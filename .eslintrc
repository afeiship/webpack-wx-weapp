{
  "extends": ["prettier", "prettier/standard"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "commonjs": true
  },
  "globals": {
    "App": true,
    "Page": true,
    "Component": true,
    "Behavior": true,
    "wx": true,
    "nx": true,
    "getApp": true
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
